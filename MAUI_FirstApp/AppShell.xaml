<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MAUI_FirstApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MAUI_FirstApp"
    Shell.FlyoutBehavior="Disabled">
 <!--
      The Default Content as of now is MainPage
        ContentTemplate: The UI Object that will be loaded
        Route: The Navivation Query Object that will be used to Navigate to teh Request ContentPage
    -->
    
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

</Shell>
